<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="232.0" prefWidth="311.0" spacing="20.0" styleClass="backimage" stylesheets="@StyleSheet.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.assignment2furtherprogramming.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Login" />
   <TextField fx:id="username" prefHeight="36.0" prefWidth="255.0" promptText="Username" />
   <PasswordField fx:id="password" promptText="Password" />
   <Button mnemonicParsing="false" onAction="#Login" prefHeight="26.0" prefWidth="78.0" text="Login" textFill="#6f6f6f" />
   <Button mnemonicParsing="false" onAction="#Signup" prefHeight="26.0" prefWidth="146.0" text="Sign Up" textFill="#6f6f6f" />
   <Text fx:id="Error" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="Error" visible="false" />
</VBox>
